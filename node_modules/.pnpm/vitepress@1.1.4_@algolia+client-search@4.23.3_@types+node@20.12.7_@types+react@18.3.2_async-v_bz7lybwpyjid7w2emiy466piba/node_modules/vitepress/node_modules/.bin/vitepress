#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/am/Workspace/am-element/node_modules/.pnpm/vitepress@1.1.4_@algolia+client-search@4.23.3_@types+node@20.12.7_@types+react@18.3.2_async-v_bz7lybwpyjid7w2emiy466piba/node_modules/vitepress/bin/node_modules:/home/am/Workspace/am-element/node_modules/.pnpm/vitepress@1.1.4_@algolia+client-search@4.23.3_@types+node@20.12.7_@types+react@18.3.2_async-v_bz7lybwpyjid7w2emiy466piba/node_modules/vitepress/node_modules:/home/am/Workspace/am-element/node_modules/.pnpm/vitepress@1.1.4_@algolia+client-search@4.23.3_@types+node@20.12.7_@types+react@18.3.2_async-v_bz7lybwpyjid7w2emiy466piba/node_modules:/home/am/Workspace/am-element/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/am/Workspace/am-element/node_modules/.pnpm/vitepress@1.1.4_@algolia+client-search@4.23.3_@types+node@20.12.7_@types+react@18.3.2_async-v_bz7lybwpyjid7w2emiy466piba/node_modules/vitepress/bin/node_modules:/home/am/Workspace/am-element/node_modules/.pnpm/vitepress@1.1.4_@algolia+client-search@4.23.3_@types+node@20.12.7_@types+react@18.3.2_async-v_bz7lybwpyjid7w2emiy466piba/node_modules/vitepress/node_modules:/home/am/Workspace/am-element/node_modules/.pnpm/vitepress@1.1.4_@algolia+client-search@4.23.3_@types+node@20.12.7_@types+react@18.3.2_async-v_bz7lybwpyjid7w2emiy466piba/node_modules:/home/am/Workspace/am-element/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress.js" "$@"
fi
